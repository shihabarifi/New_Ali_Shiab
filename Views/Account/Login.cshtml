@model LoginViewModel
@{ 
    Layout = null;
}


<!DOCTYPE html>

<html lang="en" class="light-style customizer-hide" dir="ltr" data-theme="theme-default" data-assets-path="~/accounts/assets/"
  data-template="vertical-menu-template">

<head>
  <meta charset="utf-8" />
  <meta name="viewport"
    content="width=device-width, initial-scale=1.0, user-scalable=no, minimum-scale=1.0, maximum-scale=1.0" />

  <title>تسجيل الدخول</title>

  <meta name="description" content="" />

  <!-- Favicon -->
  <link rel="icon" type="image/x-icon" href="~/accounts/assets/img/favicon/favicon.ico" />

  <!-- Fonts -->
  <link rel="preconnect" href="https://fonts.googleapis.com" />
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
  <link
    href="https://fonts.googleapis.com/css2?family=IBM+Plex+Sans:ital,wght@0,300;0,400;0,500;0,600;0,700;1,300;1,400;1,500;1,600;1,700&family=Rubik:ital,wght@0,300;0,400;0,500;0,600;0,700;1,300;1,400;1,500;1,600;1,700&display=swap"
    rel="stylesheet" />

  <!-- Icons -->
  <link rel="stylesheet" href="~/accounts/assets/vendor/fonts/boxicons.css" />
  <link rel="stylesheet" href="~/accounts/assets/vendor/fonts/fontawesome.css" />
  <link rel="stylesheet" href="~/accounts/assets/vendor/fonts/flag-icons.css" />

  <!-- Core CSS -->
  <link rel="stylesheet" href="~/accounts/assets/vendor/css/rtl/core.css" class="template-customizer-core-css" />
  <link rel="stylesheet" href="~/accounts/assets/vendor/css/rtl/theme-default.css" class="template-customizer-theme-css" />
  <link rel="stylesheet" href="~/accounts/assets/css/demo.css" />
  
  <!-- Vendors CSS -->
  <link rel="stylesheet" href="~/accounts/assets/vendor/libs/perfect-scrollbar/perfect-scrollbar.css" />
  <link rel="stylesheet" href="~/accounts/assets/vendor/libs/typeahead-js/typeahead.css" />
  <!-- Vendor -->
  <link rel="stylesheet" href="~/accounts/assets/vendor/libs/formvalidation/dist/css/formValidation.min.css" />
  
  <!-- Page CSS -->
  <!-- Page -->
  <link rel="stylesheet" href="~/accounts/assets/vendor/css/pages/page-auth.css" />
  <link rel="stylesheet" href="~/accounts/css/main.css" />
  <!-- Helpers -->
  <script src="~/accounts/assets/vendor/js/helpers.js"></script>

  <!--! Template customizer & Theme config files MUST be included after core stylesheets and helpers.js in the <head> section -->
  <!--? Template customizer: To hide customizer set displayCustomizer value false in config.js.  -->
  <script src="~/accounts/assets/vendor/js/template-customizer.js"></script>
  <!--? Config:  Mandatory theme config file contain global vars & default theme options, Set your preferred theme option in this file.  -->
  <script src="~/accounts/assets/js/config.js"></script>
</head>

<body dir="rtl">
  <!-- Content -->
 
  <div class="container-xxl">
    <div class="authentication-wrapper authentication-basic container-p-y">
     
        <!-- Register -->
       <div class="card">
          <div class="card-body">
            <h4 class="mb-2">النظام المالي</h4>
            <p class="mb-4">قم بتسجيل الدخول</p>

<div class="container IndexDiv">

            <section>
                
             <form asp-action="Login" asp-controller="Account" method="post">
                    @if(ViewBag.ErrorLogin == false)
                        {<span class="alert alert-danger w-100">
                            @ResourceWeb.lbMsgErrorLogin</span><br /> <br />}

                    <div class="input-group mb-3">
                        <input type="email" class="form-control" asp-for="Eamil" placeholder="Email">
                        <div class="input-group-append">
                            <div class="input-group-text">
                                <span class="fas fa-envelope"></span>
                            </div>
                        </div>
                    </div>
                    <span class="text-danger" asp-validation-for="Eamil"></span>

                    <div class="input-group mb-3">
                        <input type="password" class="form-control" asp-for="Password" placeholder="Password">
                        <div class="input-group-append">
                            <div class="input-group-text">
                                <span class="fas fa-lock"></span>
                            </div>
                        </div>
                    </div>
                    <span class="text-danger" asp-validation-for="Password"></span>

                    <div class="row">
                        <div class="col-8">
                            <div class="icheck-primary">
                                <input type="checkbox" asp-for="RememberMy" id="remember">
                                <label for="remember">
                                    Remember Me
                                </label>
                            </div>
                        </div>
                        <!-- /.col -->
                        <div class="col-4">
                            <button type="submit" class="btn btn-primary btn-block btn-flat">Sign In</button>
                        </div>
                        <!-- /.col -->
                    </div>
                </form>
            </section>
              
        </div>
        <div style="width:400px" class="col-4">
            <img src="~/images/sh.gif" class="img-fluid" />
        </div>

        </div>



    </div>

</div>
    </div>

</div>
        <!-- /Register -->
    
  <!-- / Content -->

  <!-- Core JS -->
  <!-- build:js ~/accounts/assets/vendor/js/core.js -->
  <script src="~/accounts/assets/vendor/libs/jquery/jquery.js"></script>
  <script src="~/accounts/assets/vendor/libs/popper/popper.js"></script>
  <script src="~/accounts/assets/vendor/js/bootstrap.js"></script>
  <script src="~/accounts/assets/vendor/libs/perfect-scrollbar/perfect-scrollbar.js"></script>
  <script src="~/accounts/assets/vendor/libs/hammer/hammer.js"></script>
  <script src="~/accounts/assets/vendor/libs/i18n/i18n.js"></script>
  <script src="~/accounts/assets/vendor/libs/typeahead-js/typeahead.js"></script>

  <script src="~/accounts/assets/vendor/js/menu.js"></script>
  <!-- endbuild -->

  <!-- Vendors JS -->
  <script src="~/accounts/assets/vendor/libs/formvalidation/dist/js/FormValidation.min.js"></script>
  <script src="~/accounts/assets/vendor/libs/formvalidation/dist/js/plugins/Bootstrap5.min.js"></script>
  <script src="~/accounts/assets/vendor/libs/formvalidation/dist/js/plugins/AutoFocus.min.js"></script>

  <!-- Main JS -->
  <script src="~/accounts/assets/js/main.js"></script>

  <!-- Page JS -->
  <script src="~/accounts/assets/js/pages-auth.js"></script>
</body>

</html>




