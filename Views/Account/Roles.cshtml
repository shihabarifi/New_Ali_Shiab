                                </thead>
                                <tbody>
                                    @foreach (var item in Model.Roles)
                                    {
                                        <tr>
                                            <td>
                                                @item.NormalizedName
                                            </td>
                                            <td>
                                                @switch (@item.Name)
                                                {
                                                    case "SupperAdmin":
                                                        <span class="badge bg-danger" style="width:250px; height:25px">@item.Name</span>
                                                         
                                                        break;
                                                    case "Admin":
                                                        <span class="badge bg-success" style="width:250px; height:25px">@item.Name</span>
                                                        break;
                                                    default:
                                                        <span class="badge bg-warning" style="width:250px; height:25px">@item.Name</span>
                                                        break;
                                                }
                                            </td>
                                            <td>
                                                <input type="button" class="btn btn-info btn-sm" onclick="Edit('@item.Id', '@item.Name')" value="@ResourceWeb.lbbtnEdit" data-bs-toggle="modal" data-bs-target="#exampleModal" />
                                                <input type="button" class="btn btn-danger btn-sm" onclick="Delete('@item.Id')" value="@ResourceWeb.lbbtnDelete" />
                                            </td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                        <!-- /.card-body -->
                    </div>
                    <!-- /.card -->
